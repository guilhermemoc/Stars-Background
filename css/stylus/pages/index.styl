body
  width: 100%
  height: auto
  background-color: #000;
  #sec
    display: block
    overflow: hidden
    max-width: 100%
    max-height: 100vh
    .stars
      width: 100%
      height: 100vh
      z-index: 99
      max-width: 100%
      max-height: 100vh
      overflow: hidden
      .star
        position: absolute;
        border-radius: 50%
        overflow: hidden
        max-width: 100%
        max-height: 100vh






